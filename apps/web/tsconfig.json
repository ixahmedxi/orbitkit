{
  "extends": ["@orbitkit/tsconfig/next.json"],
  "compilerOptions": {
    "tsBuildInfoFile": "tsconfig.tsbuildinfo",
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "**/*.js",
    "**/*.cjs",
    "**/*.mjs"
  ],
  "exclude": ["node_modules", "playwright-report", "test-results"]
}
