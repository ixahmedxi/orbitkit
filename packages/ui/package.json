{
  "name": "@orbitkit/ui",
  "version": "0.3.2",
  "private": true,
  "description": "OrbitKit UI Design System Components Library",
  "license": "MIT",
  "author": "OrbitKit",
  "sideEffects": false,
  "type": "module",
  "exports": {
    "./*": {
      "import": {
        "types": "./dist/primitives/*/index.d.ts",
        "default": "./dist/primitives/*/index.js"
      },
      "require": {
        "types": "./dist/primitives/*/index.d.cts",
        "default": "./dist/primitives/*/index.cjs"
      }
    },
    "./cn": {
      "import": {
        "types": "./dist/utils/cn.d.ts",
        "default": "./dist/utils/cn.js"
      },
      "require": {
        "types": "./dist/utils/cn.d.cts",
        "default": "./dist/utils/cn.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "build-storybook": "storybook build",
    "clean": "bun run rm -rf dist .tsup storybook-static *.tsbuildinfo",
    "dev": "tsup --watch",
    "lint": "eslint .  --max-warnings 0",
    "storybook": "storybook dev -p 6006 --no-open",
    "test-storybook": "test-storybook",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.6.0",
    "@radix-ui/react-accordion": "^1.1.2",
    "@radix-ui/react-alert-dialog": "^1.0.5",
    "@radix-ui/react-aspect-ratio": "^1.0.3",
    "@radix-ui/react-avatar": "^1.0.4",
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-collapsible": "^1.0.3",
    "@radix-ui/react-context-menu": "^2.1.5",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-hover-card": "^1.0.7",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-menubar": "^1.0.4",
    "@radix-ui/react-navigation-menu": "^1.1.4",
    "@radix-ui/react-popover": "^1.0.7",
    "@radix-ui/react-progress": "^1.0.3",
    "@radix-ui/react-radio-group": "^1.1.3",
    "@radix-ui/react-scroll-area": "^1.0.5",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-separator": "^1.0.3",
    "@radix-ui/react-slider": "^1.1.2",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-switch": "^1.0.3",
    "@radix-ui/react-toast": "^1.1.5",
    "@radix-ui/react-toggle": "^1.0.3",
    "@radix-ui/react-toggle-group": "^1.0.4",
    "@radix-ui/react-tooltip": "^1.0.7",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "cmdk": "^1.0.0",
    "date-fns": "^3.6.0",
    "embla-carousel-autoplay": "^8.1.5",
    "embla-carousel-react": "^8.1.5",
    "input-otp": "^1.2.4",
    "react": "^18.3.1",
    "react-day-picker": "^8.10.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.51.5",
    "react-resizable-panels": "^2.0.19",
    "tailwind-merge": "^2.3.0",
    "vaul": "^0.9.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@microsoft/api-extractor": "^7.47.0",
    "@orbitkit/assets": "workspace:^",
    "@orbitkit/eslint": "workspace:^",
    "@orbitkit/storybook": "workspace:^",
    "@orbitkit/tailwind": "workspace:^",
    "@orbitkit/tsconfig": "workspace:^",
    "@orbitkit/utils": "workspace:^",
    "@orbitkit/vite": "workspace:^",
    "@storybook/addon-a11y": "^8.1.7",
    "@storybook/addon-essentials": "^8.1.7",
    "@storybook/addon-interactions": "^8.1.7",
    "@storybook/addon-links": "^8.1.7",
    "@storybook/addon-themes": "^8.1.7",
    "@storybook/react": "^8.1.7",
    "@storybook/test": "^8.1.7",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "autoprefixer": "^10.4.19",
    "playwright": "^1.44.1",
    "postcss": "^8.4.38",
    "storybook": "^8.1.7",
    "tailwindcss": "^3.4.4",
    "tsup": "^8.1.0",
    "vite": "^5.2.13"
  }
}
