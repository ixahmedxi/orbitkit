{
  "name": "@orbitkit/vite",
  "version": "0.4.0",
  "private": true,
  "description": "Vite presets for the workspace",
  "license": "MIT",
  "author": "OrbitKit",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
