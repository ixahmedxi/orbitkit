{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Next.js",
  "extends": "./dom.json",
  "compilerOptions": {
    "jsx": "preserve",
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [
    "${configDir}/next-env.d.ts",
    "${configDir}/.next/types/**/*.ts",
    "${configDir}/src/**/*.ts",
    "${configDir}/src/**/*.tsx",
    "${configDir}/src/**/*.cts",
    "${configDir}/src/**/*.ctsx",
    "${configDir}/src/**/*.mts",
    "${configDir}/src/**/*.mtsx",
    "${configDir}/src/**/*.js",
    "${configDir}/src/**/*.jsx",
    "${configDir}/src/**/*.cjs",
    "${configDir}/src/**/*.cjsx",
    "${configDir}/src/**/*.mjs",
    "${configDir}/src/**/*.mjsx"
  ]
}
